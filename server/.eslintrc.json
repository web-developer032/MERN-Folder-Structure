{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "space-before-function-paren": "off",
    "no-empty": "error",
    "no-unused-vars": "off",
    "no-unreachable": "error",
    "no-use-before-define": "error",
    "no-unused-expressions": "error",
    "no-empty-function": "error",
    "no-return-await": "error",
    "no-useless-return": "error"
  }
}
